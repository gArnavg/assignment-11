//WAP to find the smallest missing positive element in the sorted Array that contains only positive elements.
#include<iostream>
using namespace std;
{
    int main()
  int arr[5]={12, 45, 67, 34, 43};
    bool flag = false;
    for(int i = 0;i < 5;i++)
    {
        for(int j = i++;j < 5;j++)
        {
            if(arr[i] = arr[j])
            {
                flag = true;
                cout<<arr[i];
                break;
            }
        }
    }
    if(flag==false) cout << "No duplicate”;
    return 0;
}
